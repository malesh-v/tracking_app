<header class='navbar navbar-fixed-top navbar-inverse'>
  <div class='container'>
    <%= link_to 'Tracking app', root_path, id: 'logo' %>
    <nav>
      <ul class='nav navbar-nav navbar-right'>
        <li><%= link_to 'Home', root_path  %></li>
        <li><%= link_to 'Help', help_path %></li>
        <% if signed_in? %>
          <li><%= link_to 'All Tickets', '#' %></li>
          <li><%= link_to 'Tickets assigned to me', '#' %></li>
          <% if current_staffmember.admin? %>
              <li><%= link_to 'Staffmembers', staffmembers_path %></li>
          <% end %>
          <li><%= link_to 'Sign out', signout_path, method: 'delete' %></li>
        <% else %>
          <li><%= link_to 'Log in', signin_path %></li>
        <% end %>>
      </ul>
    </nav>
  </div>
</header>